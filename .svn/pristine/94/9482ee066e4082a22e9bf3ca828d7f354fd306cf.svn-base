{yun:}include file="$wapstyle/member/cheader.htm"{/yun} 
<section class="wap_member">
<div class="wap_member_comp_h1"><span>已邀请面试的简历</span></div>
	<div class="wap_member_Receive"> 
		{yun:}if !empty($rows){/yun}
		{yun:}foreach from=$rows item=job{/yun}
		<div class="wap_member_msg ">  
		<a href="{yun:}url m=wap c=resume a=show id=$user[$job.uid].eid{/yun}" class=" com_member_hr_cblue">{yun:}$user[$job.uid].name{/yun}</a>
		<div class="com_member_hr_p1">
			<span class="member_c9">经验：</span>{yun:}$user[$job.uid].exp{/yun}   &nbsp;&nbsp;&nbsp;
			<span class="member_c9">期望薪资：</span>
			<span class="wap_member_msg_p1_cf">{yun:}$user[$job.uid].salary{/yun}</span>
		</div>
		<div>面试职位：{yun:}$job.jobname{/yun}</div>

		<i>{yun:}$job.datetime|date_format:'%Y-%m-%d'{/yun}</i>
		<em class="user_size"><a href="javascript:void(0)" onclick="layer_del('确定要删除？','index.php?c=invite_del&id={yun:}$job.id{/yun}')"  class="wap_member_msg_date">删除</a>
		{yun:}if $job.is_browse=='1'{/yun}未查看{yun:}elseif $job.is_browse=='2'{/yun}已查看{yun:}elseif $job.is_browse=='3'{/yun}已同意{yun:}elseif $job.is_browse=='4'{/yun}已拒绝{yun:}/if{/yun}
		</em> 
		 </div> 
		{yun:}/foreach{/yun}       
		<div class="wap_member_Receive_list"> 
		<div class="diggg">{yun:}$pagenav{/yun}</div>
		</div>
		{yun:}elseif $smarty.get.keyword!="" {/yun}
		 <div class="wap_member_no">没有搜索邀请记录。</div>
		{yun:}else{/yun}
		 <div class="wap_member_no">您还没有邀请记录。</div>
		{yun:}/if{/yun}  
	</div> 
</section>     
<div class="clear"></div> 
 
{yun:}include file="$wapstyle/footer.htm"{/yun} 