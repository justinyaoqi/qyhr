*{outline: none; }
html,body{height:100%; margin:0; padding:0;}
body{margin:0;padding:0; font-family:微软雅黑,宋体;  color:#333;word-wrap:break-word;width:100%;height:100%; position:relative}
body, h1, h2, h3, h4, h5, h6, hr, p, dl, dt, dd, ul, ol, li, button, input, textarea, th, td {margin:0; padding:0;}
button, input, textarea, th, td { font:14px/22px 微软雅黑,宋体;}
img{border:none;}
.dn{ display:none;}
.db{display:block;}
.fl{float:left}
.fr{float:right;}
.clear{ clear:both;height: 0; line-height:0px;} 
a{color:#555; text-decoration:none;cursor:pointer;-moz-transition: background-color 0.2s ease-out,color 0.1s ease-out; -webkit-transition: background-color 0.2s ease-out,color 0.1s ease-out;-ms-transition: background-color 0.2s ease-out,color 0.1s ease-out;transition: background-color 0.2s ease-out,color 0.1s ease-out;}
a:hover{color:#f60; text-decoration:underline}
em{ font-style:normal;}
i{ font-style:normal;}
ul{margin:0;padding:0;list-style:none;}
h2{font-size:16px;margin:10px 0;}
.c9{color:#999;}
.c7{color:#777;}
.c5{color:#555;}
.c6{color:#666;}
.c3{color:#333;}
.cf{color:#fff}
.cf6{color:#00b38a;}
.cblue{color:#0d5a86}
/*字体样式*/
.f24{ font-size:24px;}
.f20{ font-size:20px;}
.f18{ font-size:18px;}
.f16{ font-size:16px;}
.f14{ font-size:14px;}
.f12{ font-size:12px;}
.mb10{margin-bottom:10px;}
.mb20{margin-bottom:20px;}
.mb0{ margin-bottom:0;}
.mb50{margin-bottom:50px;}
.mt10{ margin-top:10px;}
.mt5{ margin-top:5px;}
.mt15{ margin-top:15px;}
.mt20{ margin-top:20px;}
/*footer 样式*/
.Return_top{ position:fixed;right:30px;bottom:350px;}
.footer{width:100%;height:115px; background:#fff; margin-top:20px;}
.footer_code{width:69px; text-align:center;}
.footer_m{width:695px;border-left:1px solid #d4d4d4; padding-left:30px; margin-left:30px;_margin-left:15px;}
.footer_m_t{}
.footer_m_t a{padding-right:10px;}
.footer_m_t .footer_m_t_s{ padding-right:10px;}
.footer_m_p{ line-height:30px;}
.footer_tel{ width:180px;text-align:right;}
.footer_tel_tit{ width:100%;height:32px; line-height:32px;background:url(../images/yh_tel.jpg) no-repeat 60px;}
.regGuide{width:100%;height:56px; background:#0089af;}
.regGuide_Part{ position:relative}
.regGuideLogo{ width:217px;height:97px;position:absolute;left:-230px;top:-95px;}
.regGuideLogo_by{width:200px;height:56px;position:absolute;left:-160px;top:15px;}
.regGuide_Part_l{width:660px; text-align:right; line-height:56px;}
.regGuide_Part_l_span{ margin-left:85px;}
.regGuide_login{ line-height:56px;}
.regGuide_reg{width:138px;height:56px; display:inline-block; background:#fe7807; text-align:center; margin-left:30px;}
.regGuide_reg:hover{color:#fff;}


/*登录样式*/
.login_pop-up_box{width:747px;background:#fff; position:absolute;left:300px;top:100px; z-index:1002}
.login_pop-up_box_left{width:360px; padding:40px 0px 20px 40px;}
.login_pop-up_box_tit{width:100%; background:url(../images/recharge_line.gif) repeat-x bottom;float:left;}
.login_pop-up_box_tit li{width:100px;height:40px; line-height:40px; padding-left:50px;float:left; font-size:18px;margin-left:10px; cursor:pointer; position:relative;border-radius:5px 5px  0px  0px }
.login_pop-up_box_tit  .login_pop-up_box_tit_cur{border:1px solid #d8d8d8; border-top:3px solid #4d87b5;border-bottom:1px solid #fff;color:#1d1b1a}
.login_pop-up_box_tit .login_pop-up_box_tit_cur .login_pop-up_icon{ width:24px;height:18px;display:block; position:absolute;left:15px;top:12px; background:url(../images/lo_dl.png) no-repeat 0 0}
 .login_pop-up_icon{ width:24px;height:18px;display:block; position:absolute;left:15px;top:12px; background:url(../images/lo_dl.png) no-repeat 0 0}
.login_pop-up_box_tit .login_pop-up_icon_wx{ background:url(../images/lo_dl.png) no-repeat -40px -22px}
.login_pop-up_box_tit .login_pop-up_box_tit_cur .login_pop-up_icon_wx{ background:url(../images/lo_dl.png) no-repeat -40px 0}
.login_pop-up_box_bor{width:290px;height:41px;border:1px solid #dad8d8; padding-left:50px; background:url(../images/lo_p1.jpg) no-repeat left 5px; overflow:hidden}
.login_pop-up_box_bor_pbg{background:url(../images/lo_p2.jpg) no-repeat left 5px}
.login_pop-up_box_in{ width:270px;padding:8px;height:25px; line-height:25px;border:none;}
.login_pop-up_box_t1{width:340px; position:relative}
.login_pop-up_fw{ position:absolute;right:0px;top:0px;color:#0086b5}
.login_pop-up_box_bth{width:330px;height:50px; background:#0086b5;border:none; font-size:18px;color:#fff;border-radius:5px; cursor:pointer}
.login_pop-up_box_bth:hover {background-color: #08749a;}
.login_pop-up_box_right{width:270px; padding:100px 0px 100px 32px; background:url(../images/login_bigbg.png) no-repeat; position:relative}
.login_pop-up_box_right .login_close{width:33px;height:33px; background:url(../images/lo_close.png) no-repeat 0 0; overflow:hidden; display:block; position:absolute;right:10px;top:10px;}
.login_pop-up_box_right .login_close:hover{ background:url(../images/lo_close.png) no-repeat -48px 0; }
.login_pop-up_qq,.login_pop-up_sina{width: 238px;border: 1px solid #a5dcfa;color: #fff;line-height: 50px;height: 50px;border-radius: 10px;margin-bottom: 17px;text-align: center;font-size: 14px;cursor: pointer; display:block;}
.login_pop-up_qq:hover {background: #2ed0ff;border-color: #2ed0ff;color:#fff; text-decoration:none}
.login_pop-up_sina:hover {background: #ff7575;border-color: #ff7575;color:#fff;text-decoration:none}
.login_pop-up_qq  i ,.login_pop-up_sina i {background: url(../images/log_all_img.png) no-repeat;width: 26px;height: 29px;display: inline-block;margin-right: 9px;vertical-align: middle;margin-top: -2px;background-position: 0px 0px;}
.login_pop-up_qq:hover i{background-position: -40px 0px;}
.login_pop-up_sina   i {width: 34px;height: 27px;background-position: 0px -30px;}
.login_pop-up_sina:hover i{background-position: -40px -30px;}
.login_pop-up_no{width:238px; text-align:right;color:#fff}
.login_pop-up_reg{color:#6cd0ff}













