<script>
$(document).ready(function(e) {
    $("#addcoll").click(function(){
		$("#topcoll").hide();
		var colltype=$("#colltype").val();
		$("."+colltype+"id").show();
		$("#type").val(colltype);
		$(".allid").show();
	})
	$("input[name=urltype]").click(function(){
		var value=$(this).val();
		if(value==1){
			$("#url_1").show();
			$("#url_2").hide();
		}else{
			$("#url_1").hide();
			$("#url_2").show();
		}
	})
	$("input[name=password_type]").click(function(){
		var value=$(this).val();
		if(value==3){
			$("#passwordid").show();
		}else{
			$("#passwordid").hide();
		}
	})
	if("{yun:}$row.name{/yun}"!=""){
	$("#addcoll").click();
	}
});

	$(document).ready(function(e) {
		$("#job_provinceid_mr").change(function(){
			var job1=$(this).val();
			if(job1==0 || job1==""){
				$('#job_cityid_mr option').remove();
				$('#job_three_cityid_mr option').remove();
				$('<option value="0">默认为空</option>').appendTo("#job_cityid_mr");
				$('<option value="0">默认为空</option>').appendTo("#job_three_cityid_mr");
				return false;
			}
			$.post("index.php?m=collection&c=ajaxgetcity",{id:job1,pytoken:$('#pytoken').val()},function(data){
				if(data){
					$('#job_cityid_mr option').remove();$(data).appendTo("#job_cityid_mr");$("#job_cityid_mr").change();
				}else{
					$('<option value="0">默认为空</option>').appendTo("#job_cityid_mr");
					$('<option value="0">默认为空</option>').appendTo("#job_three_cityid_mr");
				}
			});
		}) 
		$("#job_cityid_mr").change(function(){
			var job1_son=$(this).val();
			if(job1_son==0 || job1_son==""){
				$('#job_three_cityid_mr option').remove();
				$('<option value="0">默认为空</option>').appendTo("#job_three_cityid_mr");return false;}
			$.post("index.php?m=collection&c=ajaxgetcity",{id:job1_son,pytoken:$('#pytoken').val()},function(data){
				if(data!=""){
					$('#job_three_cityid_mr option').remove();$(data).appendTo("#job_three_cityid_mr");
				}else{
					$('<option value="0">默认为空</option>').appendTo("#job_three_cityid_mr");
				}
			});
		}) 
	});
</script>
