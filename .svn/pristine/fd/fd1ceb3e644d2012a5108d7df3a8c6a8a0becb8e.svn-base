/*内容页*/
.topic-content{width: 100%;overflow:hidden;}

.topic-content .topic-doc {
    margin-bottom: 30px;
	overflow: hidden;
}
.topic-content .topic-doc h3 {
    background: none repeat scroll 0 0 transparent;
    height: auto;
    margin: 0;
    padding: 5px 0;
	
margin-bottom: 1px;
margin-left: 4px;
color: #666666;
    font: 14px/150% Arial;
}

.topic-content .topic-doc img{max-width:500px;};

.color-green {
    color: #006600;
}

.pl20 {
    padding-left: 20px;
}
.topic-content .topic-doc p {
    margin: 0;
    overflow: hidden;
    padding: 0;
    word-wrap: break-word;
	line-height: 23px;
}
.topic-content .topic-opt {
    padding: 15px 0 0;
}
.rec-sec {
    color: #999999;
    text-align: right;
}
.rec-num {
    margin: 0 4px;
}
.clearfix {
    display: block;
}

/*评论*/
.topic-reply{width: 100%;overflow:hidden;}
.topic-reply li {
    margin: 0 0 20px 0px;
	overflow:hidden;
}
.topic-content .user-face, .topic-reply .user-face {
    float: left;
    height: 48px;
    overflow: hidden;
    width: 48px;
}

.topic-reply li .reply-doc {
    float: right;
    width: 520px;
	overflow: hidden;
}

.topic-reply li .reply-doc p img{max-width:450px;}

.bg-img-green {
    background: url("topicbar.gif") no-repeat scroll 0 0 transparent;
    display: block;
}

.topic-reply li .reply-doc h4 {
    height: 22px;
    line-height: 22px;
    margin: 0 0 15px 4px;
}

h4 {
    background: url("topicbar.gif") no-repeat scroll right top transparent;
    color: #666666;
    font: 12px/1.8em Arial;
    height: 26px;
    margin: 0 0 10px 4px;
    overflow: hidden;
}

.topic-reply li .reply-doc p {
