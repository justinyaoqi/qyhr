}
.bn-std1, a.bn-std1:hover, .bn-std1 span {
    background: url("bg_bn_red.png") no-repeat scroll 0 0 transparent;
    display: inline-block;
    height: 20px;
    overflow: hidden;
}

.bn-std1 span {
    background-position: 100% 0;
    color: #524942;
    height: 20px;
    line-height: 20px;
    margin-left: 3px;
    overflow: hidden;
    padding: 0 10px;
}

/*发表按钮*/
a.bn-create, a.bn-post {
    background: url("post.gif") no-repeat scroll 0 0 transparent;
    color: #9E9E9E;
    padding-left: 17px;
}

.bn-create, .bn-create span, .bn-post, .bn-post span {
    display: -moz-inline-stack;
    font-size: 0;
    height: 22px;
    letter-spacing: 4px;
    line-height: 0;
    overflow: hidden;
    padding-left: 3px;
    vertical-align: middle;
}

.bn-create, .bn-create span, .bn-post, .bn-post span, x:default {
    display: inline-block;
}

