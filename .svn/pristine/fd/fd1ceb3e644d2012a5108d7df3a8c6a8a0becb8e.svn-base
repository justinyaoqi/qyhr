	overflow:hidden;
}

.input_all{border:solid 1px #ccc;padding:3px;}
.textarea_all{border:solid 1px #ccc;padding:3px;overflow:hidden;}

.f_login{margin:50px auto;width:500px;}
.f_reg{margin:30px auto;width:600px;}


.tb_list{padding:10px 0;}
